var util = require('util');//常用函数模块
function Base() {
    this.name = 'base';
    this.base = '1991';
    this.sayHello = function() {
        console.log('Hello' + this.name);
    };
}
Base.prototype.showName = function() {
    console.log(this.name);
}
function Sub() {
    this.name = 'sub';
}

util.inherits(Sub,Base);//Sub 继承 Base
var sub = new Sub();
var base = new Base();
console.log(base);
console.log(sub);
